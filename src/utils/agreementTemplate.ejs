  function withTime(inputDateString) {
    // Create a Date object from the input string
    const date = new Date(inputDateString);
  
    // Array of month abbreviations
    const monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
    const month = monthNames[date.getUTCMonth()]; // Get month name
  
    // Get the day and year
    const day = date.getUTCDate();
    const year = date.getUTCFullYear();
  
    // Get the UTC time components
    const hours = date.getUTCHours();
    const minutes = date.getUTCMinutes();
  
    // Function to get the appropriate suffix for the day
    function getDaySuffix(day) {
      if (day > 3 && day < 21) return 'th'; // Handle teens
      switch (day % 10) {
        case 1: return 'st';
        case 2: return 'nd';
        case 3: return 'rd';
        default: return 'th';
      }
    }
  
    const dayWithSuffix = `${day}${getDaySuffix(day)}`;
  
    // Format the time with leading zeros
    const formattedTime = `${hours < 10 ? '0' + hours : hours}:${minutes < 10 ? '0' + minutes : minutes} UTC`;
  
    // Return the formatted date and time
    return `${month} ${dayWithSuffix}, ${year} at ${formattedTime}`;
  }
  
function withoutTime(inputDateString) {
    // Create a Date object from the input string
    const date = new Date(inputDateString);
  
    // Array of month abbreviations
    const monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
    const month = monthNames[date.getMonth()]; // Get month name
  
    // Get the day and year
    const day = date.getDate();
    const year = date.getFullYear();
  
    // Function to get the appropriate suffix for the day
    function getDaySuffix(day) {
      if (day > 3 && day < 21) return 'th'; // Handle teens
      switch (day % 10) {
        case 1: return 'st';
        case 2: return 'nd';
        case 3: return 'rd';
        default: return 'th';
      }
    }
  
    const dayWithSuffix = `${day}${getDaySuffix(day)}`;
  
    // Return the formatted date
    return `${month} ${dayWithSuffix}, ${year}`;
  }

